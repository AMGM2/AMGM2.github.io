disp(' Ejercicio 2.7.9 ')
disp(' Considere el siguiente conjunto de datos: ')
disp('(x1,y1) = [[2;3],4],(x2,y2) = [[3;3],3]')
disp('(x3,y3) = [[2;4],3],(x4,y4) = [[3;4],5]')
disp('Estimar los valores de los parámetros mediante el método de mínimo cuadrados, así como el estimado para x = [5;6].')

format short % formato corto con 4 dígitos después del punto decimal.
X = [1,2,3;1,3,3;1,2,4;1,3,4] % datos de entrenamiento del vector X
Y = [4;3;3;5] % datos de entrenamiento del vector Y
beta_estimado = (X'*X)^(-1)*X'*Y
disp(beta_estimado)
Y_estimado=beta_estimado(1,1)*5+beta_estimado(2,1)*6
disp(Y_estimado)
